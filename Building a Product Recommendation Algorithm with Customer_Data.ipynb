    "    today = date.today()\n",
    "    return today.year - born.year - ((today.month, today.day) < (born.month, born.day))\n",
    "  \n",
    "df2['Age'] = df2['Date of Birth'].apply(age)\n",
    "  \n",
    "# display(df2)"
